    "type": "library",
    "authors": [
        {
            "name": "SÃ©bastien L'haire",
            "email": "sebastien@lhaire.org",
            "homepage": "http://sebastien.lhaire.org/"
        }
    ],
    "homepage": "https://github.com/seblhaire/tablebuilder",
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "illuminate/http": "^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Seblhaire\\TableBuilder\\": "src/"
        }
    },
    "extra": {
      "laravel": {
          "providers": [
              "Seblhaire\\TableBuilder\\TableBuilderHelperServiceProvider"
